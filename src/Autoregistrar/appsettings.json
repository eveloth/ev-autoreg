{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },

  "ConnectionStrings": {
    "Default": ""
  },

  "SymmetricSecurityKey" : "",

  "Serilog": {

    "Using": [ "serilog.Sinks.File" ],

    "MinimumLevel": {
      "Default": "Information"
    },

    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "../logs/autoregistrar-.log",
          "rollingInterval": "Day",
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz}] {CorrelationId} {Level:u3} {Username} {Message:lj}{Exception}{NewLine}"
        }
      },
      {
        "Name": "Seq",
        "Args": {
          "ServerUrl": ""
        }
      }
    ]
 }
}