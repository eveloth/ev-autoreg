{
  "Serilog": {

    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Warning",
        "System": "Warning"
      }
    },

    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "../logs/ev-autoreg-.log",
          "rollingInterval": "Day",
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz}] {CorrelationId} {Level:u3} {Username} {Message:lj}{Exception}{NewLine}"
        }
      },
      {
        "Name": "Seq",
        "Args": {
          "ServerUrl": "",
          "ApiKey": ""
        }
      }
    ]
  },
  
  "ConnectionStrings": {
    "Default": "",
    "Redis": "",
    "RedisCache" : ""
  },
  
  "Jwt": {
    "Issuer": "",
    "Key": "",
    "Lifetime": "",
    "RefreshTokenLifetime": ""
  },

  "RedisOptions": {
    "RefreshTokenDb": ""
  },

  "RedisCacheOptions": {
    "Enabled" : ""
  },

  "SymmetricSecurityKey" : "",

  "AutoregistrarUri": ""
}