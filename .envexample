# DATABASE

PG_DB='ev_autoreg_db'
PG_USER='autoregpguser'
PG_PASS=''

# CRYPTOGRAPHY

SYMMETRICSECURITYKEY=''

# JWT

JWT__ISSUER='evautoreg'
JWT__KEY=''
JWT__LIFETIME='00:15:00'
JWT__REFRESHTOKENLIFETIME='168:00:00'

# SERILOG

SERILOGOPTIONS__ENABLECONSOLE="true"
SERILOGOPTIONS__ENABLEFILE="false"
SERILOGOPTIONS__ENABLESEQ="false"

SERILOG__MINIMUMLEVEL__DEFAULT='Information'

# SEQ

SEQOPTIONS__SERVERURL=''
SEQOPTIONS__APIKEY=''

# REDIS

REDISOPTIONS__PASSWORD=''
REDISOPTIONS__REFRESHTOKENDB=2

# CACHE

REDISCACHEOPTIONS__ENABLED="true"

# AUTOREGISTRAR

AUTOREGISTRARURI='evautoreg.autoregistrar'

# SWAGGER AND GRPC REFLECTION

SWAGGEROPTIONS__ENABLED="false"
GRPCREFLECTIONOPTIONS__ENABLED="false"

# NETWORKING

# API_HOST=127.0.0.1
API_PORT=7444
# AUTOREG_HOST=127.0.0.1
AUTOREG_PORT=7445
# POSTGRES_HOST=127.0.0.1
POSTGRES_PORT=5432
# REDIS_HOST=127.0.0.1
REDIS_PORT=6379


# CONNECTION STRINGS

CONNECTIONSTRINGS__DEFAULT="Server=evautoreg.postgres;Port=5432;Database=${PG_DB};User Id=${PG_USER};Password=${PG_PASS};"
CONNECTIONSTRINGS__REDIS='evutoreg.redis'
CONNECTIONSTRINGS__REDISCACHE='evutoreg.redis'

